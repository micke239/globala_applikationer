<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
  xmlns:composite="http://java.sun.com/jsf/composite"
  xmlns:h="http://java.sun.com/jsf/html"
  xmlns:c="http://java.sun.com/jsp/jstl/core">

    <h:head>
        <title>This content will not be displayed</title>
    </h:head>
    <h:body>
        <composite:interface>
            <composite:attribute name="head" required="false"/>
            <composite:attribute name="user" required="false"/>
            <composite:attribute name="realusername" required="false"/>
        </composite:interface>

        <composite:implementation>
            <h3><h:outputLabel value="#{cc.attrs.head}"></h:outputLabel></h3>
        <br/>
            <c:if test="#{index.isloggedin}">
                <h:outputLabel value="#{str.welcome} #{index.realname}"></h:outputLabel>
                <h:form>
                <h:commandButton value="#{str.logout}" action="#{index.doLogout}"/>
                </h:form>
            </c:if>

           
        </composite:implementation>
    </h:body>
</html>